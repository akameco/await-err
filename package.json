{
  "name": "golangify",
  "version": "0.3.1",
  "description": "Golang like error handling for async/await",
  "license": "MIT",
  "repository": "akameco/golangify",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "lint": "eslint .",
    "test": "jest",
    "test:cov": "jest --coverage --ci --runInBand",
    "test:ci": "yarn lint && yarn test:cov"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "proxy",
    "await",
    "golang",
    "async",
    "error"
  ],
  "dependencies": {},
  "devDependencies": {
    "all-contributors-cli": "6.8.1",
    "eslint": "6.1.0",
    "eslint-config-precure": "4.18.1",
    "husky": "3.0.2",
    "jest": "24.8.0",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
